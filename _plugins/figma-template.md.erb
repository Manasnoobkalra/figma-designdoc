---
layout: page
title: <%= pagetitle %>
id: <%= filename %>
<% if @figmalinks %>figmalink: https://www.figma.com/file/<%= figmadoc %>?node-id=<%= pageid %><% end %>
<%= "summary: #{summary}" if !summary.blank? %>
<%= "tags: #{tags}" if !tags.blank? %>
---

<% blocks.each do |block| %>
<%= "## #{block[0]}" if !block[0].blank? %>
<%= "![#{block[0]}](#{block[1]})" %>

<%= block[2] %>
<% end %>